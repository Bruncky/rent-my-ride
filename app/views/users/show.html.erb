
<h1>Your Bookings</h1>
<% current_user.bookings.each do |booking| %>
<div class="card rounded" style="background-color: rgba(245, 245, 245, 0.5)">
  <%= cl_image_tag booking.car.photo.key, width: 300; %>
   <div class="card-body"><%= booking.car.model %>
      <p><%= booking.start_date %> - <%= booking.end_date %></p>
      <%= link_to "", car_path(booking.car), class: "fas fa-info-circle" %>
    </div>
  </div>
<% end %>
